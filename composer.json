{
    "name": "mapexss/filament-env-editor",
    "description": "Access .env file though Filament admin panel",
    "keywords": [
        "mapexss",
        "laravel",
        "laravel-env-editor",
        "EnvEditor",
        "filament-env-editor"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mapexss"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "mapexss/laravel-env-editor": "^3.0",
        "illuminate/contracts": ">=10.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": ">=9",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mapexss\\FilamentEnvEditor\\": "src/"
        }
    },
    "scripts": {
        "phpstan": "php --version && php vendor/bin/phpstan --version && php -d memory_limit=1G vendor/bin/phpstan analyse -c ruleset-phpstan.neon  -vvv",
        "cs": "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots --config=ruleset-php_cs.php",
        "all": [
            "@phpstan",
            "@cs"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mapexss\\FilamentEnvEditor\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
